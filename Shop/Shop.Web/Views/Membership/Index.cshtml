@{
    ViewBag.Title = "Membership";
}

<h2>Your registry data</h2>
<span>@ViewBag.message</span>

<form method="post" action="/Membership/CreateCustomer">
    <div class="form-group">
        <label for="exampleInputName">Forename </label>
        <input type="text" class="form-control" name="forename" aria-describedby="ForenameHelp" placeholder="Enter Forename">
        <small id="NameHelp" class="form-text text-muted">We'll never share your confidential data with anyone else.</small>
    </div>

    <div class="form-group">
        <label for="exampleInputName">Surename </label>
        <input type="text" class="form-control" name="Surename" aria-describedby="SurenameHelp" placeholder="Enter Surename">
    </div>

    <div class="form-group">
        <label for="exampleInputName">Address </label>
        <input type="text" class="form-control" name="address" aria-describedby="AddressHelp" placeholder="Enter Address">
    </div>

    <div class="form-group">
        <label for="exampleInputName">Emai </label>
        <input type="text" class="form-control" name="email" aria-describedby="EmailHelp" placeholder="Enter Email">
    </div>

    <div class="form-group">
        <label for="exampleInputName">Phone Number </label>
        <input type="text" class="form-control" name="phone" aria-describedby="PhoneHelp" placeholder="Enter Phone Number">
    </div>

    <div class="form-group">
        <label for="exampleInputPassword1">Password</label>
        <input type="password" class="form-control" name="password" placeholder="Password">
    </div>

    <div class="form-group">
        <label for="exampleInputName">Date of birth </label>
        <input type="date" class="form-control" name="Date of birth" aria-describedby="Help" placeholder="Enter date of birth">
    </div>

    <button type="submit" class="btn btn-primary">Submit</button>
</form>
